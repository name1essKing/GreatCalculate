<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="360" d:DesignHeight="633"
             x:Class="GreatCalculate.Client.Views.Tabs.TaskManagerTabView">
	<Grid Margin="40">
		<CheckBox VerticalAlignment="Top" HorizontalAlignment="Left"
				  FontWeight="SemiLight"
				  IsChecked="False"/>
		<CheckBox VerticalAlignment="Center" HorizontalAlignment="Left"
				  FontWeight="SemiLight"
				  IsChecked="{Binding TaskManagerMark}"/>
	</Grid>
</UserControl>

